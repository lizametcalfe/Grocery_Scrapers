				if(window.location.host == 'www.tesco.com' || window.location.host == 'tesco.com') {
				var tesco_cookie_widget = document.getElementById('tesco_cookie_widget');
				var results = document.cookie.match ( '(^|;) ?tesco_cookie_accepted=([^;]*)(;|$)' );
				if (!results) {
				document.getElementById('tesco_cookie_widget').style.display = "inline";
				window.onload = function() {
				for (var i = 0, L = document.links.length; i < L; i++) {
				var link_href = document.links[i].getAttribute('href');
				if ('privacy' == document.links[i].getAttribute('id')) {
				document.links[i].onclick = function() {
				var win = new TESCO.system.Window("/termsandconditions/privacy.htm", "help", "menubar=yes,width=990,height=600,toolbar=no,scrollbars=yes,status=no,resizable=yes,location=yes,titlebar=yes");
				win.open();
				}
				} else {
				if (!/^[\w]+:/.test(link_href) || (new RegExp('^[\\w]+://[\\w\\d\\-\\.]*' + window.location.host)).test(link_href)) {
				var current_onclick = document.links[i].onclick;
				document.links[i].onclick = function() {tesco_cookie_accept();if (Object.prototype.toString.call(current_onclick) == '[object Function]') {current_onclick();}};
				}
				}
				}
				}
				}
				}
				function tesco_cookie_accept() {
				document.cookie = 'tesco_cookie_accepted=1; path=/; expires=Mon, 18 Jan 2038 01:23:45 GMT;';
